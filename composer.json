{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "awssat/laravel-visits": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jiannei/laravel-response": "^4.0",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.12",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "overtrue/laravel-subscribe": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.7",
        "brainmaestro/composer-git-hooks": "^2.8",
        "doctrine/dbal": "^3.2",
        "facade/ignition": "^2.17",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "./bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "cghooks update",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-merge": "composer install",
        "post-install-cmd": [
            "cghooks add --ignore-lock",
            "cghooks update"
        ],
        "cghooks": "vendor/bin/cghooks",
        "check-style": "php-cs-fixer fix --using-cache=no --diff --config=.php-cs-fixer.php --dry-run --ansi",
        "fix-style": "php-cs-fixer fix --using-cache=no --config=.php-cs-fixer.php --ansi",
        "test": "@php artisan test"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        },
        "hooks": {
            "pre-commit": [
                "composer test",
                "composer check-style"
            ],
            "pre-push": [
                "composer test",
                "composer check-style"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
